    "jest": "20.0.4",
    "js-cookie": "^2.2.0",
    "moment": "^2.18.1",
    "object-assign": "4.1.1",
    "papaparse": "^4.6.3",
    "polished": "^3.6.4",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dev-utils": "^3.1.0",
    "react-dom": "^16.13.1",
    "react-error-overlay": "^1.0.10",
    "react-google-maps": "^9.4.5",
    "react-i18next": "^11.8.4",
    "react-redux": "^7.2.0",
